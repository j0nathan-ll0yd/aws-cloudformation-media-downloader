{
  "name": "aws-cloudformation-media-downloader",
  "main": "./node_modules/webpack-cli/bin/cli.js",
  "version": "1.0.0",
  "engines": {
    "node": "v16.16.0",
    "npm": "9.6.4"
  },
  "scripts": {
    "check-types": "./node_modules/typescript/bin/tsc --project ./tsconfig.json",
    "coverage": "AWS_REGION=us-west-2 ./node_modules/nyc/bin/nyc.js npm run test",
    "document-source": "./bin/document-source.sh",
    "document-terraform": "terraform-docs markdown terraform/ > docs/terraform.md",
    "build": "./node_modules/webpack-cli/bin/cli.js",
    "install-prod": "npm install --only=production",
    "build-terraform-json": "cat terraform/*.tf > build/terraform.tf && hcl2json < build/terraform.tf > build/terraform.json",
    "build-terraform-types": "quicktype ./build/terraform.json -o ./src/types/terraform.d.ts",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .ts .",
    "lint-fix": "./node_modules/eslint/bin/eslint.js --ext .ts . --fix",
    "plop": "./node_modules/plop/bin/plop.js",
    "test": "npm run test-setup-config && npm run test-setup-mocha",
    "test-full": "npm run build && npm run build-terraform-json && npm run build-terraform-types && npm run test",
    "test-debug": "TS_NODE_PROJECT=\"tsconfig.testing.json\" DEBUG_LEVEL=TRACE MOCHA_DEBUG=1 AWS_REGION=us-west-2 ./node_modules/mocha/bin/mocha.js --require ts-node/register --config config/.mocharc.yml",
    "test-pipeline": "DEBUG_LEVEL=DEBUG act -s AWS_ACCESS_KEY_ID=$(aws configure get aws_access_key_id) -s AWS_SECRET_ACCESS_KEY=$(aws configure get aws_secret_access_key) -s GITHUB_TOKEN",
    "test-remote-list": "./bin/test-list.sh",
    "test-remote-hook": "./bin/test-hook.sh",
    "test-remote-loginUser": "./bin/test-loginUser.sh",
    "test-remote-registerDevice": "./bin/test-registerDevice.sh",
    "test-remote-userSubscribe": "./bin/test-userSubscribe.sh",
    "test-setup-config": "cat tsconfig.json | sed 's/\"module\": \"es6\"/\"module\": \"commonjs\"/g' tsconfig.json > tsconfig.testing.json",
    "test-setup-mocha": "TS_NODE_PROJECT=\"tsconfig.testing.json\" AWS_REGION=us-west-2 ./node_modules/mocha/bin/mocha.js --require ts-node/register --config config/.mocharc.yml",
    "deploy": "cd terraform && terraform apply -auto-approve"
  },
  "author": "Jonathan Lloyd",
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "@octokit/rest": "^19.0.13",
    "apn": "^2.2.0",
    "aws-sdk": "^2.1406.0",
    "aws-xray-sdk": "^3.5.0",
    "axios": "^1.4.0",
    "debug": "^4.3.4",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "uuid": "^9.0.0",
    "validate.js": "^0.13.1",
    "yaml": "^2.3.1",
    "ytdl-core": "^4.11.4"
  },
  "sideEffects": false,
  "bundleDependencies": [],
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@kie/mock-github": "^1.1.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/debug": "^4.1.8",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.2",
    "@types/sinon": "^10.0.15",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "aws-sdk-mock": "^5.8.0",
    "axios-mock-adapter": "^1.21.5",
    "babel-loader": "^9.1.2",
    "benny": "^3.7.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "optionator": "^0.9.1",
    "plop": "^3.1.2",
    "quicktype": "^17.0.6",
    "redoc": "^2.0.0",
    "sinon": "^15.2.0",
    "source-map-support": "^0.5.21",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.5",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "node-fetch@<2.6.7": "2.6.7",
    "yargs-parser@>=5 <13.1.2": "13.1.2",
    "yargs-parser@>=14 <15.0.1": "15.0.1",
    "yargs-parser@>=16 <18.1.2": "18.1.2"
  }
}
