{
  "name": "aws-cloudformation-media-downloader",
  "version": "1.0.0",
  "engines": {
    "node": "v22.18.0",
    "npm": ">=10.9.0"
  },
  "type": "module",
  "scripts": {
    "check-types": "./node_modules/typescript/bin/tsc --project ./tsconfig.json",
    "codegraph-typescript": "npx tsx ./bin/codegraph.ts > ./build/graph.json",
    "document-source": "./bin/document-source.sh",
    "document-terraform": "terraform-docs markdown terraform/ > docs/terraform.md",
    "document-api": "./bin/document-api.sh",
    "build": "node --loader ts-node/esm ./node_modules/.bin/webpack-cli --config ./config/webpack.config.ts",
    "build-dependencies": "./bin/build-dependencies.sh",
    "install-prod": "npm install --only=production",
    "format": "./node_modules/.bin/prettier --write 'src/**/*.{ts,js,json}' 'config/**/*.{ts,js,json}'",
    "lint": "./node_modules/.bin/eslint -c ./eslint.config.mjs .",
    "lint-fix": "./node_modules/.bin/eslint -c ./eslint.config.mjs . --fix",
    "plop": "./node_modules/plop/bin/plop.js",
    "test": "node --no-warnings --experimental-vm-modules ./node_modules/.bin/jest --silent --config config/jest.config.mjs",
    "test-full": "npm run build-dependencies && npm run build && npm run test",
    "test-pipeline": "DEBUG_LEVEL=DEBUG act -s AWS_ACCESS_KEY_ID=$(aws configure get aws_access_key_id) -s AWS_SECRET_ACCESS_KEY=$(aws configure get aws_secret_access_key) -s GITHUB_TOKEN",
    "test-remote-list": "./bin/test-list.sh",
    "test-remote-hook": "./bin/test-hook.sh",
    "update-cookies": "./bin/update-youtube-cookies.sh",
    "update-yt-dlp": "./bin/update-yt-dlp.sh",
    "plan": "eval export $(cat .env) && cd terraform && tofu plan",
    "deploy": "eval export $(cat .env) && cd terraform && tofu apply -auto-approve"
  },
  "author": "Jonathan Lloyd",
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.716.0",
    "@aws-sdk/client-cloudwatch": "^3.932.0",
    "@aws-sdk/client-dynamodb": "^3.716.0",
    "@aws-sdk/client-lambda": "^3.930.0",
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/client-secrets-manager": "^3.716.0",
    "@aws-sdk/client-sfn": "^3.716.0",
    "@aws-sdk/client-sns": "^3.716.0",
    "@aws-sdk/client-sqs": "^3.716.0",
    "@aws-sdk/lib-dynamodb": "^3.716.0",
    "@aws-sdk/lib-storage": "^3.930.0",
    "@aws-sdk/util-dynamodb": "^3.716.0",
    "@babel/runtime": "^7.26.0",
    "@octokit/rest": "^21.0.2",
    "apns2": "^12.0.0",
    "axios": "^1.12.0",
    "joi": "^17.13.3",
    "jose": "^5.9.6",
    "jwks-rsa": "^3.1.0",
    "uuid": "^11.0.3",
    "yt-dlp-wrap": "^2.3.12",
    "ytdl-core": "^4.11.5"
  },
  "sideEffects": false,
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jest/globals": "^29.7.0",
    "@types/aws-lambda": "^8.10.146",
    "@types/debug": "^4.1.12",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@typespec/compiler": "^1.5.0",
    "@typespec/http": "^1.5.0",
    "@typespec/json-schema": "^1.5.0",
    "@typespec/openapi": "^1.5.0",
    "@typespec/openapi3": "^1.5.0",
    "babel-loader": "^9.2.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "esm": "^3.2.25",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^11.0.0",
    "jest": "^29.7.0",
    "plop": "^3.1.2",
    "quicktype": "^23.1.3",
    "redoc": "^2.2.0",
    "source-map-support": "^0.5.21",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-morph": "^25.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
