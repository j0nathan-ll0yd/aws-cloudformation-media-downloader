{
  "name": "aws-cloudformation-media-downloader",
  "main": "./node_modules/webpack-cli/bin/cli.js",
  "version": "1.0.0",
  "engines": {
    "node": "v14.16.1",
    "npm": "8.3.0"
  },
  "scripts": {
    "check-types": "tsc",
    "coverage": "AWS_REGION=us-west-2 ./node_modules/nyc/bin/nyc.js npm run test",
    "document-source": "./bin/document-source.sh",
    "document-terraform": "terraform-docs markdown terraform/ > docs/terraform.md",
    "build": "./node_modules/webpack-cli/bin/cli.js",
    "install-prod": "npm install --only=production",
    "build-terraform-json": "cat terraform/*.tf > build/terraform.tf && docker run -i tmccombs/hcl2json < build/terraform.tf > build/terraform.json",
    "build-terraform-types": "quicktype ./build/terraform.json -o ./src/types/terraform.d.ts",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .ts .",
    "lint-fix": "./node_modules/eslint/bin/eslint.js --ext .ts . --fix",
    "test": "npm run test-setup-config && npm run test-setup-mocha",
    "test-full": "npm run build && npm run build-terraform-json && npm run build-terraform-types && npm run test",
    "test-debug": "DEBUG_LEVEL=TRACE AWS_REGION=us-west-2 ./node_modules/mocha/bin/mocha",
    "test-pipeline": "DEBUG_LEVEL=DEBUG act -s AWS_ACCESS_KEY_ID=$(aws configure get aws_access_key_id) -s AWS_SECRET_ACCESS_KEY=$(aws configure get aws_secret_access_key) -s GITHUB_TOKEN",
    "test-remote-list": "./bin/test-list.sh",
    "test-remote-hook": "./bin/test-hook.sh",
    "test-remote-loginUser": "./bin/test-loginUser.sh",
    "test-remote-registerDevice": "./bin/test-registerDevice.sh",
    "test-remote-userSubscribe": "./bin/test-userSubscribe.sh",
    "test-setup-config": "cat tsconfig.json | sed 's/\"module\": \"es6\"/\"module\": \"commonjs\"/g' tsconfig.json > tsconfig.testing.json",
    "test-setup-mocha": "TS_NODE_PROJECT=\"tsconfig.testing.json\" AWS_REGION=us-west-2 ./node_modules/mocha/bin/mocha",
    "deploy": "cd terraform && terraform apply -auto-approve"
  },
  "author": "Jonathan Lloyd",
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "aws-sdk": "^2.1063.0",
    "aws-xray-sdk": "^3.3.4",
    "axios": "^0.25.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1",
    "yaml": "^1.10.2",
    "ytdl-core": "^4.10.0"
  },
  "sideEffects": false,
  "bundleDependencies": [],
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/aws-lambda": "^8.10.92",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.12",
    "@types/sinon": "^10.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "aws-sdk-mock": "^5.6.0",
    "axios-mock-adapter": "^1.20.0",
    "babel-loader": "^8.2.3",
    "benny": "^3.7.1",
    "chai": "^4.3.5",
    "chai-as-promised": "^7.1.1",
    "debug-level": "^2.1.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "mocha": "^9.2.0",
    "nock": "^13.2.2",
    "nyc": "^15.1.0",
    "optionator": "^0.9.1",
    "quicktype": "^15.0.240",
    "redoc": "^2.0.0-rc.61",
    "sinon": "^11.1.1",
    "source-map-support": "^0.5.21",
    "terser-webpack-plugin": "^5.3.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.11",
    "typedoc-plugin-banish": "^1.0.1",
    "typedoc-plugin-not-exported": "^0.1.6",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2"
  }
}
