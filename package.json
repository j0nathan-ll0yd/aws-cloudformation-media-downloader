{
  "name": "aws-cloudformation-media-downloader",
  "version": "1.0.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.27.0"
  },
  "packageManager": "pnpm@10.27.0",
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true pnpm run build && node --import tsx scripts/visualize-bundles.ts && open build/reports/bundle-analysis.html",
    "audit": "pnpm audit --audit-level=high",
    "audit:aws": "./bin/aws-audit.sh",
    "audit:fix": "pnpm audit --fix",
    "audit:wiki": "./bin/audit-wiki-docs.sh",
    "build": "pnpm run generate:graph && node --import tsx config/esbuild.config.ts",
    "build:bgutil:layer": "./bin/build-bgutil-layer.sh",
    "build:deno:layer": "./bin/build-deno-layer.sh",
    "build:dependencies": "./bin/build-dependencies.sh",
    "check:bundle:size": "node --import tsx scripts/check-bundle-size.ts",
    "check:secrets": "./bin/check-secrets.sh",
    "check:test:types": "tsc --project ./tsconfig.test.json --noEmit",
    "check:types": "tsc --project ./tsconfig.json",
    "ci:local": "./bin/ci-local.sh",
    "ci:local:full": "./bin/ci-local-full.sh",
    "cleanup": "./bin/cleanup.sh",
    "cleanup:check": "./bin/cleanup.sh --check",
    "cleanup:fast": "./bin/cleanup.sh --fast",
    "dashboard:conventions": "node --import tsx scripts/generateConventionDashboard.ts",
    "db:export": "drizzle-kit export --sql --dialect postgresql",
    "db:generate": "drizzle-kit generate",
    "deploy": "./bin/pre-deploy-check.sh && eval export $(cat .env) && cd terraform && tofu apply -auto-approve",
    "deploy:check": "./bin/pre-deploy-check.sh",
    "deploy:force": "eval export $(cat .env) && cd terraform && tofu apply -auto-approve",
    "deps:check": "dependency-cruiser src --config .dependency-cruiser.cjs",
    "deps:graph": "dependency-cruiser src --config .dependency-cruiser.cjs --output-type dot | dot -T svg > dependency-graph.svg",
    "deps:report": "dependency-cruiser src --config .dependency-cruiser.cjs --output-type html --output-to dependency-report.html",
    "document:api": "./bin/document-api.sh",
    "document:source": "./bin/document-source.sh",
    "document:terraform": "terraform-docs markdown terraform/ > docs/terraform.md",
    "extract:cookies:chrome": "node scripts/extract-youtube-cookies-chrome.mjs",
    "extract:db-permissions": "node --import tsx scripts/extractDbPermissions.ts",
    "generate:dsql-permissions": "node --import tsx scripts/generateDsqlPermissions.ts",
    "dsql:sync": "pnpm run extract:db-permissions && pnpm run generate:dsql-permissions",
    "extract:fixtures": "./bin/extract-fixtures.sh",
    "extract:fixtures:production": "./bin/extract-production-fixtures.sh",
    "format": "dprint fmt",
    "format:bash": "shfmt -i 2 -ci -sr -w bin/*.sh",
    "format:bash:check": "shfmt -i 2 -ci -sr -d bin/*.sh",
    "format:check": "dprint check",
    "gen:api:types": "node --import tsx scripts/generateApiTypes.ts",
    "generate:graph": "node --experimental-strip-types scripts/generateDependencyGraph.ts",
    "generate:llms": "cat README.md docs/wiki/**/*.md > docs/llms-full.txt",
    "graphrag:extract": "node --import tsx graphrag/extract.ts",
    "graphrag:query": "pnpm dlx tsx graphrag/query.ts",
    "graphrag:validate": "node --import tsx graphrag/validate-metadata.ts",
    "index:codebase": "node --import tsx scripts/indexCodebase.ts",
    "install:prod": "pnpm install --prod",
    "lint": "eslint -c ./eslint.config.mjs .",
    "lint:bash": "shellcheck bin/*.sh .github/scripts/*.sh",
    "lint:fix": "eslint -c ./eslint.config.mjs . --fix",
    "lint:workflows": "actionlint",
    "localstack:health": "curl -s http://localhost:4566/_localstack/health | jq",
    "localstack:logs": "docker compose -f docker-compose.localstack.yml logs -f localstack",
    "localstack:start": "docker compose -f docker-compose.localstack.yml up -d",
    "localstack:stop": "docker compose -f docker-compose.localstack.yml down",
    "mcp:inspect": "pnpm exec mcp-inspector src/mcp/server.ts",
    "mcp:server": "node --import tsx src/mcp/server.ts",
    "pack:context": "repomix",
    "pack:light": "repomix --include \"src/**/*.ts,docs/**/*.md,AGENTS.md\" --ignore \"**/*.test.ts,**/*.spec.ts\"",
    "plan": "eval export $(cat .env) && cd terraform && tofu plan",
    "precheck": "pnpm run check:types && pnpm run lint && pnpm run format:check",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "process:fixtures": "node --import tsx scripts/processFixtures.ts",
    "report:gaps": "node --import tsx scripts/reportEnforcementGaps.ts",
    "scaffold:test": "node --import tsx scripts/scaffoldTest.ts",
    "search:codebase": "node --import tsx scripts/searchCodebase.ts",
    "state:verify": "./bin/verify-state.sh",
    "test": "export LOG_LEVEL=${LOG_LEVEL:-SILENT} && pnpm run --silent generate:graph && vitest run --config vitest.config.mts",
    "test:eslint:rules": "node eslint-local-rules/test/no-direct-aws-sdk-import.test.cjs && node eslint-local-rules/test/cascade-delete-order.test.cjs && node eslint-local-rules/test/use-entity-mock-helper.test.cjs && node eslint-local-rules/test/response-helpers.test.cjs && node eslint-local-rules/test/env-validation.test.cjs && node eslint-local-rules/test/enforce-powertools.test.cjs && node eslint-local-rules/test/no-domain-leakage.test.cjs && node eslint-local-rules/test/strict-env-vars.test.cjs && node eslint-local-rules/test/migrations-safety.test.cjs && node eslint-local-rules/test/import-order.test.cjs",
    "test:integration": "export LOG_LEVEL=${LOG_LEVEL:-SILENT} POWERTOOLS_LOG_LEVEL=${POWERTOOLS_LOG_LEVEL:-SILENT} NODE_NO_WARNINGS=1 && pnpm run --silent generate:graph && USE_LOCALSTACK=true vitest run --config vitest.integration.config.mts",
    "test:mutation": "stryker run",
    "test:mutation:auth": "stryker run --mutate 'src/lambdas/{ApiGatewayAuthorizer,LoginUser,RegisterUser,RefreshToken}/**/src/**/*.ts'",
    "test:mutation:incremental": "stryker run --incremental",
    "test:remote:hook": "./bin/test-hook.sh",
    "test:remote:list": "./bin/test-list.sh",
    "test:remote:register:device": "./bin/test-registerDevice.sh",
    "typespec:check": "tsp compile tsp/ --no-emit",
    "typespec:compile": "tsp compile tsp/",
    "update:agents:prs": "./bin/update-agents-prs.sh",
    "update:ffmpeg": "./bin/update-ffmpeg.sh",
    "update:ffmpeg:check": "./bin/update-ffmpeg.sh --check-only",
    "update:ytdlp": "./bin/update-yt-dlp.sh",
    "update:ytdlp:check": "./bin/update-yt-dlp.sh --check-only",
    "validate:api:paths": "node --import tsx scripts/validateApiPaths.ts",
    "validate:config": "node --import tsx scripts/validateConfig.ts",
    "validate:conventions": "node --import tsx scripts/validateConventions.ts",
    "validate:doc:sync": "./bin/validate-doc-sync.sh",
    "validate:docs": "./bin/validate-docs.sh",
    "validate:docs:freshness": "./bin/validate-docs-freshness.sh",
    "validate:graphrag": "./bin/validate-graphrag.sh"
  },
  "author": "Jonathan Lloyd",
  "dependencies": {
    "@aws-lambda-powertools/idempotency": "^2.30.0",
    "@aws-lambda-powertools/logger": "^2.30.0",
    "@aws-lambda-powertools/metrics": "^2.30.0",
    "@aws-sdk/client-api-gateway": "3.962.0",
    "@aws-sdk/client-dynamodb": "3.962.0",
    "@aws-sdk/client-eventbridge": "3.962.0",
    "@aws-sdk/client-lambda": "3.962.0",
    "@aws-sdk/client-s3": "3.962.0",
    "@aws-sdk/client-sns": "3.962.0",
    "@aws-sdk/client-sqs": "3.962.0",
    "@aws-sdk/dsql-signer": "3.962.0",
    "@aws-sdk/lib-dynamodb": "3.962.0",
    "@aws-sdk/lib-storage": "3.962.0",
    "@middy/core": "^6.4.5",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@octokit/rest": "^22.0.1",
    "@opentelemetry/api": "^1.9.0",
    "apns2": "12.2.0",
    "better-auth": "1.4.10",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "postgres": "^3.4.7",
    "zod": "^4.3.4"
  },
  "sideEffects": false,
  "imports": {
    "#entities/*": "./src/entities/*",
    "#lambdas/*": "./src/lambdas/*",
    "#lib/*": "./src/lib/*",
    "#util/*": "./src/util/*",
    "#config/*": "./src/config/*",
    "#types/*": "./src/types/*",
    "#test/*": "./test/*"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@lancedb/lancedb": "^0.23.0",
    "@modelcontextprotocol/inspector": "^0.18.0",
    "@redocly/cli": "^2.12.7",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/typescript-checker": "^9.4.0",
    "@stryker-mutator/vitest-runner": "^9.4.0",
    "@types/aws-lambda": "^8.10.159",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@typespec/compiler": "^1.8.0",
    "@typespec/http": "^1.8.0",
    "@typespec/json-schema": "^1.8.0",
    "@typespec/openapi": "^1.8.0",
    "@typespec/openapi3": "^1.8.0",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-vitest": "^7.0.1",
    "dependency-cruiser": "^17.3.5",
    "dprint": "^0.51.1",
    "drizzle-kit": "^0.31.8",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-tsdoc": "^0.5.0",
    "fastembed": "^2.1.0",
    "glob": "^13.0.0",
    "husky": "^9.1.7",
    "mock-jwks": "^3.3.0",
    "puppeteer-core": "^24.6.0",
    "quicktype": "^23.2.6",
    "repomix": "^1.11.0",
    "ts-morph": "^27.0.2",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "vite": "^7.0.0",
    "vitest": "^4.0.16",
    "vitest-monocart-coverage": "^4.0.1"
  },
  "pnpm": {
    "overrides": {
      "qs": ">=6.14.1",
      "drizzle-zod>zod": "^4.3.4",
      "esbuild": ">=0.25.0",
      "undici-types": "7.16.0",
      "hono@<4.11.4": ">=4.11.4",
      "diff@<8.0.3": ">=8.0.3",
      "undici@<6.23.0": ">=6.23.0",
      "undici@>=7.0.0 <7.18.2": ">=7.18.2",
      "tar@<=7.5.2": ">=7.5.3"
    }
  }
}
