{
  "name": "lifegames-fileviewer",
  "version": "1.0.0",
  "engines": {
    "node": "8.1.x",
    "npm": "5.0.x"
  },
  "scripts": {
    "check-types": "tsc",
    "coverage": "AWS_REGION=us-west-2 ./node_modules/nyc/bin/nyc.js npm run test",
    "build": "./node_modules/@babel/cli/bin/babel.js src --out-dir dist --extensions '.ts'",
    "test": "AWS_REGION=us-west-2 ./node_modules/mocha/bin/mocha",
    "test-remote-list": "./bin/test-list.sh",
    "test-local-list": "sam local invoke \"ListFiles\" -e swagger/empty.json",
    "test-remote-hook": "./bin/test-hook.sh",
    "test-local-hook": "sam local invoke \"FeedlyWebhook\" -e swagger/event.json",
    "deploy-code": "./bin/update-lambda-functions.sh",
    "deploy-node-modules": "./bin/update-node-modules.sh",
    "deploy-cloudformation": "./bin/update-cloudformation.sh",
    "test-dev": "nodemon --watch . --ext ts --exec \"mocha -r ts-node/register test/**/*.test.ts\""
  },
  "keywords": [
    "lifegames"
  ],
  "author": "Jonathan Lloyd",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@types/chai": "^4.2.5",
    "aws-sdk": "^2.269.1",
    "aws-xray-sdk": "^2.3.3",
    "axios": "^0.18.1",
    "clear-require": "^3.0.0",
    "redoc": "^2.0.0-rc.10",
    "request": "^2.87.0",
    "urlsafe-base64": "^1.0.0",
    "validate.js": "^0.12.0",
    "ytdl-core": "^1.0.0"
  },
  "bundleDependencies": [],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/aws-lambda": "^8.10.7",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.5",
    "aws-sdk-mock": "^4.5.0",
    "axios-mock-adapter": "^1.17.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^6.0.3",
    "mocha": "^5.2.0",
    "mock-aws-sinon": "^1.3.2",
    "nock": "^10.0.0",
    "nodemon": "^1.19.4",
    "npm-pack-zip": "^1.2.7",
    "nyc": "^13.3.0",
    "proxyquire": "^2.1.3",
    "rewiremock": "^3.13.9",
    "sinon": "^6.3.4",
    "sinon-as-promised": "^4.0.3",
    "source-map-support": "^0.5.16",
    "spectacle-docs": "^1.0.7",
    "ts-node": "^8.5.2",
    "tslint": "^5.10.0",
    "typescript": "^3.7.2"
  }
}
