{
  "name": "lifegames-sandbox",
  "version": "1.0.0",
  "engines": {
    "node": "10.16.3",
    "npm": "6.13.x"
  },
  "scripts": {
    "check-types": "tsc",
    "coverage": "AWS_REGION=us-west-2 ./node_modules/nyc/bin/nyc.js npm run test",
    "build": "./node_modules/@babel/cli/bin/babel.js src --out-dir dist --extensions '.ts'",
    "test": "AWS_REGION=us-west-2 ./node_modules/mocha/bin/mocha",
    "test-remote-list": "./bin/test-list.sh",
    "test-local-list": "npm run build && sam local invoke -t cloudformation.json --env-vars env.json \"ListFiles\"",
    "test-remote-hook": "./bin/test-hook.sh",
    "test-remote-registerDevice": "./bin/test-registerDevice.sh",
    "test-remote-dispatchNotification": "./bin/test-dispatchNotification.sh",
    "test-local-hook": "npm run build && sam local invoke -t cloudformation.json --env-vars env.json \"FeedlyWebhook\" -e test/fixtures/handleFeedlyEvent/APIGatewayEvent.json",
    "test-local-schedule": "npm run build && sam local invoke -t cloudformation.json --env-vars env.json \"FeedlyWebhook\" -e test/fixtures/handleFeedlyEvent/CloudWatchEventRuleEvent.json",
    "deploy-code": "./bin/update-lambda-functions.sh",
    "deploy-node-modules": "./bin/update-node-modules.sh",
    "deploy-cloudformation": ". ./bin/environment-setup.sh && ./bin/predeploy-cloudformation.sh && ./node_modules/cfpack.js/bin/cfpack.js deploy",
    "destroy-cloudformation": ". ./bin/environment-setup.sh && ./node_modules/cfpack.js/bin/cfpack.js delete"
  },
  "author": "Jonathan Lloyd",
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "aws-sdk": "^2.630.0",
    "aws-xray-sdk": "^2.5.0",
    "axios": "^0.18.1",
    "clear-require": "^3.0.0",
    "only": "0.0.2",
    "urlsafe-base64": "^1.0.0",
    "validate.js": "^0.13.1",
    "ytdl-core": "^2.0.0"
  },
  "bundleDependencies": [],
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/aws-lambda": "^8.10.39",
    "@types/chai": "^4.2.7",
    "@types/chai-as-promised": "^7.1.2",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.7.7",
    "@types/sinon": "^7.5.1",
    "aws-sdk-mock": "^4.5.0",
    "axios-mock-adapter": "^1.17.0",
    "benny": "^3.6.14",
    "cfpack.js": "^1.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.2.2",
    "mock-aws-sinon": "^1.3.2",
    "nock": "^11.7.0",
    "nyc": "^15.0.0-beta.3",
    "redoc": "^2.0.0-rc.20",
    "sinon": "^7.5.0",
    "sinon-as-promised": "^4.0.3",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  }
}
