{
  "name": "aws-cloudformation-media-downloader",
  "main": "./node_modules/webpack-cli/bin/cli.js",
  "version": "1.0.0",
  "engines": {
    "node": "v12.16.1",
    "npm": "7.12.0"
  },
  "scripts": {
    "check-types": "tsc",
    "coverage": "AWS_REGION=us-west-2 ./node_modules/nyc/bin/nyc.js npm run test",
    "build": "./node_modules/webpack-cli/bin/cli.js",
    "install-prod": "npm install --only=production",
    "build-terraform-json": "cat terraform/*.tf > build/terraform.tf && docker run -i tmccombs/hcl2json < build/terraform.tf > build/terraform.json",
    "build-terraform-types": "./node_modules/quicktype/dist/cli/index.js ./build/terraform.json -o ./src/types/terraform.d.ts",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .ts .",
    "lint-fix": "./node_modules/eslint/bin/eslint.js --ext .ts . --fix",
    "test": "AWS_REGION=us-west-2 ./node_modules/mocha/bin/mocha",
    "test-debug": "DEBUG_LEVEL=TRACE AWS_REGION=us-west-2 ./node_modules/mocha/bin/mocha",
    "test-pipeline": "DEBUG_LEVEL=DEBUG act -s AWS_ACCESS_KEY_ID=$(aws configure get aws_access_key_id) -s AWS_SECRET_ACCESS_KEY=$(aws configure get aws_secret_access_key) -s GITHUB_TOKEN",
    "test-remote-list": "./bin/test-list.sh",
    "test-remote-hook": "./bin/test-hook.sh",
    "test-remote-loginUser": "./bin/test-loginUser.sh",
    "test-remote-registerDevice": "./bin/test-registerDevice.sh",
    "deploy": "cd terraform && terraform apply -auto-approve"
  },
  "author": "Jonathan Lloyd",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "aws-sdk": "^2.874.0",
    "aws-xray-sdk": "^3.2.0",
    "axios": "^0.21.1",
    "debug-level": "^2.0.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.2",
    "optionator": "^0.9.1",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1",
    "ytdl-core": "^4.5.0"
  },
  "bundleDependencies": [],
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/aws-lambda": "^8.10.73",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/sinon": "^9.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "aws-sdk-mock": "^5.1.0",
    "axios-mock-adapter": "^1.19.0",
    "babel-loader": "^8.2.2",
    "benny": "^3.6.15",
    "cache-loader": "^4.1.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "mocha": "^8.3.2",
    "mock-aws-sinon": "^1.3.2",
    "nock": "^13.0.11",
    "nyc": "^15.1.0",
    "quicktype": "^15.0.240",
    "redoc": "^2.0.0-rc.48",
    "sinon": "^10.0.0",
    "sinon-as-promised": "^4.0.3",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0"
  }
}
