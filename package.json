{
  "name": "aws-cloudformation-media-downloader",
  "main": "./node_modules/webpack-cli/bin/cli.js",
  "version": "1.0.0",
  "engines": {
    "node": "v20.16.0",
    "npm": "v10.8.2"
  },
  "type": "module",
  "scripts": {
    "check-types": "./node_modules/typescript/bin/tsc --project ./tsconfig.json",
    "coverage": "AWS_REGION=us-west-2 ./node_modules/nyc/bin/nyc.js npm run test",
    "document-source": "./bin/document-source.sh",
    "document-terraform": "terraform-docs markdown terraform/ > docs/terraform.md",
    "build": "TS_NODE_PROJECT=\"tsconfig.testing.json\" ./node_modules/webpack-cli/bin/cli.js",
    "build-dependencies": "./bin/build-dependencies.sh",
    "install-prod": "npm install --only=production",
    "lint": "./node_modules/.bin/eslint -c ./eslint.config.mjs .",
    "lint-fix": "./node_modules/.bin/eslint -c ./eslint.config.mjs . --fix",
    "plop": "./node_modules/plop/bin/plop.js",
    "test": "npm run test-setup-config && npm run test-setup-mocha",
    "test-full": "npm run build-dependencies && npm run build && npm run test",
    "test-debug": "TS_NODE_PROJECT=\"tsconfig.testing.json\" DEBUG_LEVEL=TRACE MOCHA_DEBUG=1 AWS_REGION=us-west-2 ./node_modules/mocha/bin/mocha.js --require ts-node/register --config config/.mocharc.yml",
    "test-pipeline": "DEBUG_LEVEL=DEBUG act -s AWS_ACCESS_KEY_ID=$(aws configure get aws_access_key_id) -s AWS_SECRET_ACCESS_KEY=$(aws configure get aws_secret_access_key) -s GITHUB_TOKEN",
    "test-remote-list": "./bin/test-list.sh",
    "test-remote-hook": "./bin/test-hook.sh",
    "test-setup-config": "cat tsconfig.json | sed 's/\"module\": \"es6\"/\"module\": \"commonjs\"/g' tsconfig.json > tsconfig.testing.json",
    "test-setup-mocha": "TS_NODE_PROJECT=\"tsconfig.testing.json\" AWS_REGION=us-west-2 ./node_modules/mocha/bin/mocha.js --require ts-node/register --config config/.mocharc.yml",
    "plan": "eval export $(cat .env) && cd terraform && terraform apply",
    "deploy": "eval export $(cat .env) && cd terraform && terraform apply -auto-approve"
  },
  "author": "Jonathan Lloyd",
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.623.0",
    "@aws-sdk/client-dynamodb": "^3.623.0",
    "@aws-sdk/client-s3": "^3.623.0",
    "@aws-sdk/client-secrets-manager": "^3.623.0",
    "@aws-sdk/client-sfn": "^3.623.0",
    "@aws-sdk/client-sns": "^3.623.0",
    "@aws-sdk/client-sqs": "^3.623.0",
    "@aws-sdk/lib-dynamodb": "^3.623.0",
    "@aws-sdk/smithy-client": "^3.374.0",
    "@aws-sdk/util-dynamodb": "^3.623.0",
    "@babel/runtime": "^7.25.0",
    "@octokit/rest": "^21.0.1",
    "apns2": "^11.7.0",
    "axios": "^1.7.2",
    "debug": "^4.3.6",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "uuid": "^10.0.0",
    "validate.js": "^0.13.1",
    "yaml": "^2.5.0",
    "ytdl-core": "^4.11.5"
  },
  "sideEffects": false,
  "bundleDependencies": [],
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/register": "^7.24.6",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jest/globals": "^29.7.0",
    "@kie/mock-github": "^2.0.1",
    "@types/aws-lambda": "^8.10.143",
    "@types/chai": "^4.3.16",
    "@types/chai-as-promised": "^7.1.8",
    "@types/debug": "^4.1.12",
    "@types/eslint__js": "^8.42.3",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.0.0",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0-alpha.62",
    "aws-sdk-mock": "^6.1.1",
    "axios-mock-adapter": "^1.22.0",
    "babel-loader": "^9.1.3",
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-tsdoc": "^0.3.0",
    "esm": "^3.2.25",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^11.0.0",
    "jest": "^29.7.0",
    "mocha": "^10.7.0",
    "nock": "^14.0.0-beta.6",
    "nyc": "^17.0.0",
    "optionator": "^0.9.4",
    "plop": "^4.0.1",
    "quicktype": "^23.0.170",
    "redoc": "^2.1.5",
    "sinon": "^18.0.0",
    "source-map-support": "^0.5.21",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.5",
    "typescript": "^5.2.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "node-fetch@<2.6.7": "2.6.7",
    "node-forge@<1.3.1": "1.3.1",
    "semver@<7.5.3": "7.5.3",
    "jsonwebtoken@<9.0.1": "9.0.1",
    "yargs-parser@>=5 <13.1.2": "13.1.2",
    "yargs-parser@>=14 <15.0.1": "15.0.1",
    "yargs-parser@>=16 <18.1.2": "18.1.2"
  }
}
