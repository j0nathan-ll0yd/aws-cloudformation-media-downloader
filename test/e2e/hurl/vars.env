# E2E Test Environment Variables
# Copy to vars.local.env and customize for your environment

# LocalStack endpoint (default for local testing)
BASE_URL=http://localhost:4566/restapis/test-api/prod/_user_request_

# Production/Staging endpoint (uncomment for remote testing)
# BASE_URL=https://your-api-gateway.execute-api.us-west-2.amazonaws.com/prod

# API Key (required for all requests)
API_KEY=test-api-key

# Test user credentials (generated by iOS client simulation)
# These are populated by the test setup scripts
TEST_USER_TOKEN=
TEST_USER_ID=
TEST_SESSION_ID=

# Device registration test data
TEST_DEVICE_ID=test-device-uuid-12345
TEST_DEVICE_NAME=Test iPhone
TEST_DEVICE_TOKEN=mock-apns-token-for-testing

# Feedly webhook test data
TEST_YOUTUBE_URL=https://www.youtube.com/watch?v=dQw4w9WgXcQ
